<h2><%= @user.name %>'s Practice Schedules</h2>
<% @user.practice_schedules.each do |schedule| %>
  <%= schedule.practice_hours_per_day  %> Hours in a Day
 <a href="gigs/<%= @user.id %>/gigs/<%= schedule.gig.id %>"><%= schedule.gig.date %></a>
  <%= schedule.is_completed ? 'Complete' : 'In Progress' %>
  <br>
<% end %>


<a href="gigs/new">Add new gig</a>
